{
  "name": "@nullvoxpopuli/eslint-configs",
  "version": "4.3.0",
  "description": "eslint configs for the NullVoxPopuli's projects",
  "repository": {
    "url": "https://github.com/NullVoxPopuli/eslint-configs"
  },
  "license": "MIT",
  "author": "NullVoxPopuli",
  "type": "module",
  "exports": {
    ".": "./index.js"
  },
  "imports": {
    "#utils": "./src/utils.js",
    "#types": "./src/types.ts"
  },
  "scripts": {
    "format": "prettier --write .",
    "format:all": "pnpm --filter '*' format",
    "format:check:all": "pnpm --filter '*-prettier' format:check",
    "lint:all:js": "pnpm --filter '*' lint:js",
    "lint:js": "eslint . --ignore-pattern test-packages",
    "lint:types": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "cosmiconfig": "^9.0.0",
    "ember-eslint": "^0.2.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-decorator-position": "^6.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json": "^4.0.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "prettier-plugin-ember-template-tag": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@tsconfig/node-lts": "^22.0.1",
    "@tsconfig/strictest": "^2.0.5",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.5",
    "@typescript-eslint/eslint-plugin": "8.24.1",
    "@typescript-eslint/parser": "8.24.1",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-ember": "12.5.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-qunit": "8.1.2",
    "prettier": "3.5.2",
    "release-plan": "^0.13.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  },
  "peerDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/eslint-parser": "^7.22.10",
    "@babel/plugin-proposal-decorators": "^7.22.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0 || >= 6.0.0",
    "@typescript-eslint/parser": "^5.62.0 || >= 6.0.0",
    "eslint": "^7.0.0 || ^8.0.0",
    "eslint-plugin-ember": ">= 11.10.0",
    "eslint-plugin-qunit": ">= 8.0.0",
    "prettier": "^2.8.8 || >= 3.0.0"
  },
  "peerDependenciesMeta": {
    "@babel/core": {
      "optional": true
    },
    "@babel/eslint-parser": {
      "optional": true
    },
    "@babel/plugin-proposal-decorators": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-ember": {
      "optional": true
    },
    "eslint-plugin-qunit": {
      "optional": true
    },
    "prettier": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= v16.0.0"
  },
  "volta": {
    "node": "16.20.2",
    "yarn": "1.22.22"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "ember-cli-htmlbars"
      ]
    }
  }
}
